<h1>Login</h1>
<%= form_tag '/login' do %>
<p>
  <%= label_tag :Id %><br>
  <%= text_field_tag :id %>
</p>
<p>
  <%= label_tag :Email %><br>
  <%= text_field_tag :email %>
</p>
<p>
  <%= label_tag :Password %><br>
  <%= text_field_tag :password %>
</p>

<p>
  <%= submit_tag 'Login' %>
</p>
<% end %>
